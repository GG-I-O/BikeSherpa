{
  "name": "ggio.bikesherpa.frontend",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "startHTTPS": "local-ssl-proxy --source 443 --target 8081 --cert dev.bike.local.pem --key dev.bike.local-key.pem",
    "build:apk:linux": "bash scripts/buildapk.sh",
    "build:apk:windows": "powershell -ExecutionPolicy Bypass -File .\\scripts\\buildapk.ps1",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "test": "jest --watchAll=false",
    "openAPI:web": "openapi-zod-client \"https://192.168.11.89:5287/swagger/v1/swagger.json\" -o \"./infra/openAPI/client.ts\" --implicit-required",
    "openAPI:file": "openapi-zod-client \"./infra/openAPI/swagger.json\" -o \"./infra/openAPI/client.ts\" --implicit-required"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@hookform/resolvers": "^5.2.2",
    "@legendapp/state": "^3.0.0-beta.42",
    "@microsoft/signalr": "^10.0.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@zodios/core": "^10.9.6",
    "axios": "^1.13.2",
    "expo": "~54.0.25",
    "expo-auth-session": "~7.0.9",
    "expo-blur": "^15.0.7",
    "expo-constants": "~18.0.10",
    "expo-crypto": "~15.0.7",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.9",
    "expo-network": "~8.0.7",
    "expo-router": "~6.0.15",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "inversify": "^7.10.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.66.1",
    "react-native": "0.81.5",
    "react-native-auth0": "^5.1.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-logs": "^5.5.0",
    "react-native-mmkv": "^3.3.3",
    "react-native-nitro-modules": "^0.31.9",
    "react-native-paper": "^5.14.5",
    "react-native-paper-dates": "^0.22.51",
    "react-native-paper-dropdown": "^2.3.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "reflect-metadata": "^0.2.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "29.5.14",
    "@types/react": "~19.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.4.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.13",
    "openapi-zod-client": "^1.18.3",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg)",
      "eslint.config.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/expo-env.d.ts",
      "!**/.expo/**"
    ]
  },
  "private": true
}